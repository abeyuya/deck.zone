{
  "name": "deck.zone",
  "version": "0.0.1",
  "description": "A game-creation helper for creating decks, tokens, chits, tiles, etc.",
  "repository": "https://github.com/seiyria/deck.zone",
  "license": "MIT",
  "scripts": {
    "build:app": "webpack -p --progress && cp src/index.html dist/index.html && echo 'deck.zone' >> dist/CNAME",
    "build:lang": "npm run build:lang:directives && npm run build:lang:map && nearleyc decklang/decklang.ne -o src/decklang/decklang.js && npm run build:lang:help",
    "build:lang:directives": "node gen/build-lang-directives > decklang/_directives.ne",
    "build:lang:help": "node gen/build-lang-help > src/components/help/help.json",
    "build:lang:map": "node gen/build-lang-map > src/decklang/plugins.json",
    "test:lang": "node decklang/test/testdeck",
    "test:lint": "eslint \"src/**/*.js\"",
    "test:ava": "ava --require babel-register",
    "test": "npm run build:lang && npm run test:lint && npm run test:ava && npm run test:lang",
    "start": "npm run build:lang && webpack-dev-server --open --content-base src/ --inline --hot",
    "deploy": "npm run build:app && firebase deploy"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
    "@angular/core": "^2.0.0-rc.1",
    "@angular/http": "^2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "@angular/router-deprecated": "^2.0.0-rc.1",
    "angularfire2": "^2.0.0-beta.0",
    "bootstrap": "^4.0.0-alpha.2",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "filesaverjs": "^0.2.2",
    "firebase": "^2.4.2",
    "human-readable-ids": "^1.0.1",
    "jszip": "^3.0.0",
    "lodash": "^4.11.2",
    "mathjs": "^3.2.1",
    "nearley": "^2.5.0",
    "ng2-ace": "0.0.12",
    "ng2-bootstrap": "^1.0.17",
    "ng2-fontawesome": "0.0.4",
    "ng2-storage": "0.0.2",
    "ng2-sweetalert2": "0.0.3",
    "ng2-vs-for": "^1.1.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.0",
    "eslint-config-kellyirc": "^3.1.0",
    "eslint-loader": "^1.3.0",
    "file-loader": "^0.8.5",
    "firebase-tools": "^3.0.2",
    "glob": "^7.0.3",
    "html-loader": "^0.4.3",
    "json-loader": "^0.5.3",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "nearley": "^2.5.0",
    "open-browser-webpack-plugin": "0.0.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
